{
    "name": "Eating Extension",
    "description": "Tells you to eat",
    "version": "1.0",
    "manifest_version": 3,
    "content_scripts": [
      {
        "matches": ["http://*/*", "https://*/*"],
        "js": ["app.js"]
      }
    ],
    "background": {
        "service_worker": "background.js"
    },
    "permissions": ["storage", "activeTab", "scripting", "notifications"],
    "action": {
        "default_popup": "popup.html"
    }
}